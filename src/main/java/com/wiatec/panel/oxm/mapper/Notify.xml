<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wiatec.panel.oxm.dao.NotifyDao">
    <resultMap type="com.wiatec.panel.paypal.NotifyInfo" id="notifyInfo">
        <id column="id" property="id"/>
        <result column="name" property="name"/>
        <result column="value" property="value"/>
    </resultMap>


    <insert id="insertOne" parameterType="com.wiatec.panel.paypal.NotifyInfo">
        INSERT INTO notify (name, value)
        VALUES (#{name}, #{value})
    </insert>

    <insert id="insertBath" parameterType="list">
        INSERT INTO notify (name, value) VALUES
        <foreach collection="list" item="item" separator=",">
            (#{item.name}, #{item.value})
        </foreach>
    </insert>

</mapper>